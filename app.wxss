/* 修改默认checkbox和radio样式 */
checkbox .wx-checkbox-input{
  border-radius: 50%;
  width: 38rpx;
  height: 38rpx;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked{
    border-color: #EB3C19 !important;
    background: #EB3C19 !important;
}
radio .wx-radio-input.wx-radio-input-checked::before{
    color: #fff!important;
    background:transparent;
}
/* 修改button样式 */
.medium-btn{
  width: 690rpx;
}
button[type=warn]{
  border: 1px solid #d42401!important;
  background: #eb3c19!important;
}
button[type=special]{
  border: 1px solid #eb3c19;
  color: #eb3c19; 
}


/* 填写表格样式 */
.form-block{
  background: #fff;
  margin: 20rpx 0;
  line-height: 88rpx;
}
.form-line{
  height: 88rpx;
  display: flex;
  /* padding-right:30rpx; */
  box-sizing: border-box;
  align-items: center;
  border-bottom: 1px solid #eaeaea;
  font-size: 34rpx;
  margin-left: 30rpx;
}
.form-line:last-child{
  border-bottom: none;
}
.form-line text{
  min-width: 200rpx;
  height: 88rpx;
  display: inline-block;
  box-sizing: border-box;
  flex-grow: 0;
  flex-shrink: 0
}
.form-line input{
  display: inline-block;
  height: 88rpx;
  padding-left: 30rpx;
  flex-grow: 1;
  flex-shrink: 1;
}
.form-line .placeholder-input{
  font-size: 30rpx;
  color: #bfbfbf;
}
.form-link{
  position: relative;
  display: flex;
  justify-content: space-between;
}
.form-link::after{
  position: absolute;
  content: '';
  width: 20rpx;
  height: 20rpx;
  transform: rotate(45deg);
  border-top: 1px solid #919191;
  border-right: 1px solid #919191;
  right: 30rpx;
  top: 50%;
  margin-top: -10rpx;
}
.sendCode,.disabledSend{
  width: 160rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  display: inline-block;
  font-size: 24rpx;
  border-radius: 4px;
  flex-shrink: 0;
}
.sendCode{
  border: 1px solid #eb3c19;
  color: #eb3c19;
}
.disabledSend{
  border: 1px solid #888888;
  color: #898989;
}

/* 公共样式 */
.phone{
  color: #2C52FD;
  text-decoration: underline;
}